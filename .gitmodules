[submodule "Docker"]
	path = Docker
	url = https://gitlab.com/libre-knowledge/docker.git
	ignore = dirty
[submodule "Vagrant"]
	path = Vagrant
	url = https://gitlab.com/libre-knowledge/vagrant.git
	ignore = dirty
[submodule "VirtualBox"]
	path = VirtualBox
	url = https://gitlab.com/libre-knowledge/virtualbox.git
	ignore = dirty
